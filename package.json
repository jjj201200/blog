    "description": "drowsy flesh's blog",
    "scripts": {
        "start": "egg-scripts start --port=80 --daemon",
        "start:7001": "egg-scripts start --port=7001 --daemon",
        "stop": "egg-scripts stop",
        "build:dll": "NODE_ENV=production && BABEL_ENV=production && webpack --config webpack.dll.config.js",
        "build:dev": "NODE_ENV=development && BABEL_ENV=development && webpack --config=./webpack.config.js",
        "build": "NODE_ENV=production && BABEL_ENV=production && webpack --config=./webpack.production.config.js",
        "analysis": "webpack-bundle-analyzer app/public/bundles/production.analysis.json",
        "dev": "egg-bin dev",
        "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
        "autod": "autod",
        "pkgfiles": "egg-bin pkgfiles"
    },
    "ci": {
        "version": "8, 9"
